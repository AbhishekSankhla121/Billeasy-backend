{
    "name": "Billeasy-backend",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "billeasy-backend",
    "workspaceFolder": "/workspace/Billeasy-backend/Backend",
    "shutdownAction": "none",
    "forwardPorts": [
        3000,
        8081
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "wix.vscode-import-cost",
                "meganrogge.template-string-converter",
                "esbenp.prettier-vscode",
                "xabikos.javascriptsnipp",
                "postman.postman-for-vscode"
            ]
        }
    }
}